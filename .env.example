# YOLO Satellite Detector - Environment Variables

# Servidor YOLO
YOLO_SERVER_HOST=localhost
YOLO_SERVER_PORT=5000

# Modelo YOLO (nano, small, medium, large, xlarge)
YOLO_MODEL_SIZE=nano

# Confiança mínima para detecção (0.0 - 1.0)
YOLO_CONFIDENCE_THRESHOLD=0.3

# Tamanho máximo de upload em MB
MAX_UPLOAD_SIZE=10

# Desenvolvimento
NODE_ENV=development

# URLs da aplicação
FRONTEND_URL=http://localhost:5173
API_URL=http://localhost:5000